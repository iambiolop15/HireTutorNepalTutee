            android:theme="@style/OnboardingTheme" />
        <activity
            android:name=".Dance"
            android:theme="@style/NoActionbar" />
        <activity
            android:name=".Music"
            android:theme="@style/NoActionbar" />


        <service
            android:name=".FirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <activity
            android:name=".Sports"
            android:theme="@style/NoActionbar" />
        <activity
